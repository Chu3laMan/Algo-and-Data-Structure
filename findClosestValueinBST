import java.util.*;

class Program {
  public static int findClosestValueInBst(BST tree, int target) {
    BST root = tree;
		BST current = root;
		while(current != null) {
			if(target <= current.value)
				return current.value;
			else
				return current.value;
		}
		
    return -1;
  }

  static class BST {
    public int value;
    public BST left;
    public BST right;

    public BST(int value) {
      this.value = value;
    }
  }
}
